{
  "submission": {
        "validate": "https:\/\/.*\\.connectedacademy\\.io\/#\/submission\/(.*\/\\D*)$",
        "tokens": [
            {
                "name": "course",
                "regex": "https:\/\/(.*\\.connectedacademy\\.io)\\.*"
            },
            {
                "name": "class",
                "regex": "https:\/\/.*\\.connectedacademy\\.io\/#\/submission\/(.*)\/.*"
            },
             {
                "name": "content",
                "regex": "https:\/\/.*\\.connectedacademy\\.io\/#\/submission\/.*\/(.*)"
            }
        ],
        "capture": [
            "(<script.*src.*\/dist\/4c\\.js.*?<\/script>)",
            "(<script.*?data-4c-meta[\\S\\s]*?<\/script>)",
            "(<img.*?data-4c.*?>)"
        ],
        "preview": "<img.+src\\=[\"'](.*?)[\"'].*>"
    }
  }
